  </div>

  <div class="field">
    <%= form.label :cancelled_by %>
    <%= form.number_field :cancelled_by, id: :help_session_cancelled_by %>
  </div>

  <div class="field">
    <%= form.label :message %>
    <%= form.text_field :message, id: :help_session_message %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
